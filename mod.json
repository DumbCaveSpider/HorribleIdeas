{
    "geode": "4.10.0",
    "gd": {
        "win": "2.2074",
        "android": "2.2074",
        "mac": "2.2074",
        "ios": "2.2074"
    },
    "id": "arcticwoof.horrible_ideas",
    "name": "Horrible Ideas",
    "version": "1.0.0",
    "developers": [
        "ArcticWoof",
        "Cheeseworks"
    ],
    "description": "A plethora of ways to ruin your experience...",
    "links": {
        "source": "https://github.com/DumbCaveSpider/HorribleIdeas",
        "homepage": "https://avalanche.cubicstudios.xyz/",
        "community": "https://discord.gg/gXcppxTNxC"
    },
    "issues": {
        "info": "If something isn't turning out to be as funny as we thought, please report it to the repository!",
        "url": "https://github.com/DumbCaveSpider/HorribleIdeas/issues"
    },
    "tags": [
        "joke",
        "offline",
        "universal",
        "gameplay",
        "content",
        "cheat"
    ],
    "resources": {
        "sprites": [
            "resources/*.png"
        ]
    },
    "dependencies": {
        "geode.node-ids": {
            "importance": "required",
            "version": ">=1.21.0"
        },
        "geode.custom-keybinds": {
            "importance": "required",
            "version": ">=1.10.0",
            "platforms": [
                "win",
                "mac",
                "android"
            ]
        }
    },
    "api": {
        "include": [
            "include/*.hpp"
        ]
    },
    "settings": {
        "safe-mode": {
            "type": "bool",
            "name": "Safe Mode",
            "description": "Prevent saving any progression made in levels. <cr>Keeping this enabled while using Horrible Ideas is highly recommended!</cr>",
            "default": true
        },
        "button": {
            "type": "custom:menu",
            "name": ""
        },
        "ui": {
            "type": "title",
            "name": "Interface",
            "description": "Appearance and functionality of the interface."
        },
        "floating-button": {
            "type": "bool",
            "name": "Floating Menu Button",
            "description": "Show a floating button on the main menu to quickly access the <cy>Horrible Options</c> menu. <co>Intended for mobile users.</c>",
            "default": {
                "win": false,
                "mac": false,
                "android": true,
                "ios": true
            }
        },
        "floating-button-level": {
            "type": "bool",
            "name": "Show Button in Level",
            "description": "Show the floating menu button while playing a level.",
            "default": false,
            "enable-if": "floating-button"
        },
        "floating-button-scale": {
            "type": "float",
            "name": "Floating Button Scale",
            "description": "Change the size of the floating button. <co>Only applies if <cy>Floating Menu Button</c> is enabled.</c>",
            "default": 1.0,
            "min": 0.5,
            "max": 3.0,
            "control": {
                "input": true,
                "slider": true,
                "slider-step": 0.1,
                "arrows": true,
                "arrow-step": 0.1,
                "big-arrows": true,
                "big-arrow-step": 0.5
            },
            "enable-if": "floating-button"
        },
        "floating-button-opacity": {
            "type": "int",
